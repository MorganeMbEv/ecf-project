<h3>Horaires d'ouverture</h3>

{% for openingHour in openingHours %}
    <p>{{ openingHour.day }} : {{ openingHour.lunchOpening |format_datetime('none', 'short', locale='fr') }} - {{ openingHour.lunchClosing |format_datetime('none', 'short', locale='fr') }} / {{ openingHour.dinnerOpening |format_datetime('none', 'short', locale='fr')}} - {{ openingHour.dinnerClosing |format_datetime('none', 'short', locale='fr')}} </p>
{% endfor %}
